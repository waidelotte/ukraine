{
  "IdentityOptions": {
    "IssuerUri": "http://localhost:7002",
    "CookieLifetime": "12:00:00",
    "EmitStaticAudienceClaim": true,
    "RaiseErrorEvents": true,
    "RaiseInformationEvents": true,
    "RaiseFailureEvents": true,
    "RaiseSuccessEvents": true,
    "ConfigurationSchema": "ukraine_identity_configuration",
    "OperationalSchema": "ukraine_identity_operational",
    "User": {
      "Email": {
        "RequireConfirmed": false,
        "RequireUnique": true
      },
      "Password": {
        "RequireDigit": false,
        "RequiredLength": 4,
        "RequireUppercase": false,
        "RequireLowercase": false,
        "RequireNonAlphanumeric": false
      }
    }
  },
  "UkrainePostgres": {
    "EnableSensitiveDataLogging": true,
    "EnableDetailedErrors": true,
    "MigrationsSchema" : "ukraine_identity"
  },
  "UkraineLogging": {
    "ServiceName": "service-identity",
    "MinimumLevel": "Debug",
    "Override": {
      "Microsoft": "Information"
    },
    "WriteTo": {
      "SeqServerUrl": "http://localhost:5341"
    }
  }
}