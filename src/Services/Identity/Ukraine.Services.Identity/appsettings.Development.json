{
  "IdentityOptions": {
    "IssuerUri": "http://localhost:7002",
    "CookieLifetime": "12:00:00",
    "EmitStaticAudienceClaim": true,
    "RaiseErrorEvents": true,
    "RaiseInformationEvents": true,
    "RaiseFailureEvents": true,
    "RaiseSuccessEvents": true,
    "ApiScopes": [
      {
        "Name": "API_EXAMPLE_SCOPE",
        "DisplayName": "Access to Example API"
      }
    ],
    "ApiResources": [
      {
        "Name": "service-example-api",
        "DisplayName": "Example API",
        "Scopes": [
          "API_SWAGGER_EXAMPLE_SCOPE"
        ]
      }
    ],
    "User": {
      "Email": {
        "RequireConfirmed": false,
        "RequireUnique": true
      },
      "Password": {
        "RequireDigit": false,
        "RequiredLength": 4,
        "RequireUppercase": false,
        "RequireLowercase": false,
        "RequireNonAlphanumeric": false
      }
    },
    "Clients": [
      {
        "ClientId": "service-example-api-swagger",
        "ClientName": "Example API Swagger",
        "AllowedGrantTypes": [
          "implicit"
        ],
        "AllowAccessTokensViaBrowser": true,
        "RedirectUris": [
          "http://localhost:7003/swagger/oauth2-redirect.html"
        ],
        "PostLogoutRedirectUris": [
          "http://localhost:7003/swagger/"
        ],
        "AllowedScopes": [
          "API_SWAGGER_EXAMPLE_SCOPE"
        ]
      }
    ]
  },
  "UkrainePostgres": {
    "RetryOnFailureCount": 3,
    "RetryOnFailureDelay": "0:00:05",
    "EnableSensitiveDataLogging": true,
    "EnableDetailedErrors": true
  },
  "UkraineLogging": {
    "ServiceName": "service-identity",
    "MinimumLevel": "Debug",
    "Override": {
      "Microsoft": "Information"
    },
    "WriteTo": {
      "SeqServerUrl": "http://localhost:5341"
    }
  }
}