{
  "ConnectionStrings": {
    "Postgres": "Server=localhost;Port=5432;Database=ukraine;User Id=postgres;Password=postgres;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information"
      }
    }
  },
  "CustomLogging": {
    "WriteToConsole": true,
    "WriteToSeq": true,
    "SeqServerUrl": "http://localhost:5341",
    "UseSerilog": true
  },
  "CustomTelemetry": {
    "UseZipkin": true,
    "ZipkinServerUrl": "http://localhost:9411/api/v2/spans"
  },
  "CustomHealthChecks": {
    "IsEnabled": true,
    "CheckDatabase": true
  },
  "CustomDatabase": {
    "RetryOnFailureCount": 3,
    "RetryOnFailureDelay": "0:00:05"
  }
}
