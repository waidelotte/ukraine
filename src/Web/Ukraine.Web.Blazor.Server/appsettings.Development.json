{
  "Serilog": {
	"WriteTo": [
	  {
		"Name": "Console"
	  },
	  {
		"Name": "Seq",
		"Args": {
		  "serverUrl": "http://localhost:5341"
		}
	  }
	],
	"MinimumLevel": {
	  "Default": "Debug",
	  "Override": {
		"Microsoft": "Information",
		"System": "Error"
	  }
	},
	"Enrich": [ "FromLogContext", "WithMachineName", "WithEnvironmentName", "WithProperty" ],
	"Properties": {
	  "ServiceName": "web-blazor"
	}
  }
}